{
    "name": "blacklabs/blacklabs",
    "description": "The Black Labs CLI.",
    "keywords": ["console", "cli", "blacklabs"],
    "homepage": "https://blacklabapps.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/ChaDonSom/blacklabs/issues",
        "source": "https://github.com/ChaDonSom/blacklabs"
    },
    "authors": [
        {
            "name": "Chase Somero",
            "email": "chadonsom@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "czproject/git-php": "^4.1",
        "illuminate/log": "^10.0",
        "intonate/tinker-zero": "^1.2",
        "laravel-zero/phar-updater": "^1.3",
        "nunomaduro/termwind": "^1.15.1"
    },
    "require-dev": {
        "laravel-zero/framework": "^10.0.2",
        "laravel/pint": "^1.8",
        "mockery/mockery": "^1.5.1",
        "pestphp/pest": "^2.5",
        "pestphp/pest-plugin-faker": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["builds/blacklabs"]
}
